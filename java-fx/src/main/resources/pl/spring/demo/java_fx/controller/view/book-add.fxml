    </rowConstraints>
    <children>
      <Label text="Title" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
      <Label text="Authors" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
      <TextField fx:id="bookTitleField" maxWidth="1.7976931348623157E308" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
      <TextField fx:id="bookAuthorsField" maxWidth="1.7976931348623157E308" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
    </children>
  </GridPane>
  <Button fx:id="addBookButton" defaultButton="true" layoutX="74.0" layoutY="191.0" mnemonicParsing="false" onAction="#addButtonAction" text="Add book"/>
</AnchorPane>
